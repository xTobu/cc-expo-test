{
  "cli": {
    "version": ">= 3.9.1",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "BUILD_MODE": "development"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "BUILD_MODE": "preview"
      }
    },
    "production": {
      "channel": "production",
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "BUILD_MODE": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
