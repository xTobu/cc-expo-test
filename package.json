{
  "name": "cc-expo-test",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "BUILD_MODE=development expo start",
    "start:dev": "BUILD_MODE=development expo start --dev-client --scheme=cc-expo-dev --clear",
    "build:dev": "eas build --profile=development --platform=ios --clear-cache",
    "build:test": "eas build --profile=preview --platform=ios  --clear-cache",
    "build:prod": "eas build --profile=production --platform=ios",
    "update:test": "BUILD_MODE=preview eas update --branch=preview --platform=ios",
    "update:prod": "BUILD_MODE=production eas update --branch=production --platform=ios",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "expo": "~48.0.10",
    "expo-application": "~5.1.1",
    "expo-constants": "~14.2.1",
    "expo-dev-client": "~2.2.1",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "react": "18.2.0",
    "react-native": "0.71.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
